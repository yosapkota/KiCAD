
(rules PCB PCB Design 4 Layer
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 142)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule In1.Cu
      (active off)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule In2.Cu
      (active off)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.4)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.7)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-3]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle In1.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle In2.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-3]_1000:600_um"
    (shape
      (circle F.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle In1.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle In2.Cu 1000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-3]_800:400_um" "Via[0-3]_800:400_um" default
  )
  (via 
    "Via[0-3]_1000:600_um" "Via[0-3]_1000:600_um" default
  )
  (via 
    "Via[0-3]_800:400_um-kicad_default" "Via[0-3]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-3]_1000:600_um-kicad_default" "Via[0-3]_1000:600_um" "kicad_default"
  )
  (via 
    "Via[0-3]_800:400_um-Power" "Via[0-3]_800:400_um" Power
  )
  (via 
    "Via[0-3]_1000:600_um-Power" "Via[0-3]_1000:600_um" Power
  )
  (via_rule
    default "Via[0-3]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-3]_800:400_um-kicad_default"
  )
  (via_rule
    Power "Via[0-3]_1000:600_um-Power"
  )
  (class default
    "Net-(U4-Pad7)" "Net-(U4-Pad8)" "Net-(R1-Pad1)" "Net-(U4-Pad17)" /RX /TX /D2 /D3
    /D4 /D5 /D6 /D7 /D8 /SDA /SCK /MISO
    /MOSI /RESET /ADRS1 /ADRS2 "Net-(U3-Pad1)" "Net-(U3-Pad2)" "unconnected-(U3-Pad7)" "unconnected-(U4-Pad13)"
    "unconnected-(U4-Pad14)" "unconnected-(U4-Pad19)" "unconnected-(U4-Pad22)" "unconnected-(U4-Pad25)" "unconnected-(U4-Pad26)"
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Power
    /Vcc GNDPWR
    (clearance_class Power)
    (via_rule Power)
    (rule
      (width 300.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)